export class Frequency {
  static get ONCE() { return 'once'; }
  static get DAILY() { return 'daily'; }
  static get WEEKLY() { return 'weekly'; }
  static get MONTHLY() { return 'monthly'; }
}

export const allTypesOfFrequency = [
  Frequency.ONCE,
  Frequency.DAILY,
  Frequency.WEEKLY,
  Frequency.MONTHLY
];